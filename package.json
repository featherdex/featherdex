{
	"name": "featherdex",
	"main": "index.html",
	"version": "0.0.1a",
	"description": "Trading application for the Omnifeather Decentralized eXchange",
	"author": "",
	"nodejs": true,
	"window": {
		"icon": "img/icon-256px.png",
		"width": 1280,
		"height": 800
	},
	"build": {
		"nwVersion": "0.57.1",
		"nwFlavor": "normal",
		"ffmpegIntegration": false,
		"excludes": [
			"*.log",
			"package-lock.json"
		],
		"icon": "./img/icon-256px.ico"
	},
	"scripts": {
		"start": "nw",
		"watch": "webpack --mode development --watch",
		"build": "webpack --mode production",
		"deliver": "build --tasks win-x86,win-x64,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
		"postdeliver": "rimraf ./dist/*/node_modules"
	},
	"dependencies": {
		"appdata-path": "^1.0.0",
		"async-mutex": "^0.3.2",
		"bitcoin-core": "^3.0.0",
		"crypto-js": "^4.1.1",
		"flexlayout-react": "^0.5.3",
		"lightweight-charts": "^3.6.1",
		"luxon": "^2.0.2",
		"node-fetch": "^2.6.5",
		"nw": "^0.57.1",
		"react": "^16.14.0",
		"react-app-menu": "^1.0.11",
		"react-dom": "^16.14.0",
		"react-modal": "^3.14.3",
		"react-notifications-component": "^3.0.5",
		"react-resize-aware": "^3.1.1",
		"react-search-autocomplete": "^5.4.2",
		"react-table": "^7.7.0",
		"simple-node-logger": "^21.8.12",
		"split-string": "^6.1.0",
		"terminal-in-react": "^4.3.1",
		"url-regex": "^5.0.0",
		"use-interval": "^1.3.0"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.0.2",
		"@types/lodash": "^4.14.175",
		"@types/luxon": "^2.0.5",
		"@types/node": "^16.11.0",
		"@types/node-fetch": "^2.5.12",
		"@types/react": "^17.0.30",
		"@types/react-dom": "^17.0.9",
		"@types/react-modal": "^3.13.1",
		"@types/react-notifications-component": "^3.1.1",
		"@types/react-table": "^7.7.6",
		"css-loader": "^5.2.7",
		"nwjs-builder-phoenix": "^1.15.0",
		"rimraf": "^3.0.2",
		"style-loader": "^3.3.0",
		"ts-loader": "^9.2.6",
		"url-loader": "^4.1.1",
		"webpack": "^5.58.2",
		"webpack-cli": "^4.9.0"
	}
